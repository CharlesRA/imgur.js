{
	"name": "imgur-api.js",
	"version": "2.0.0",
	"description": "An unofficial powerful library for interacting with the Imgur API",
	"main": "index.js",
	"scripts": {
		"test": "mocha",
		"test:album": "mocha ./test/Album.test.js",
		"test:account": "mocha ./test/Account.test.js",
		"test:comment": "mocha ./test/Comment.test.js",
		"test:gallery": "mocha ./test/Gallery.test.js",
		"test:image": "mocha ./test/Image.test.js",
		"format": "prettier --write '**/*.js*'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CharlesRA/imgur.js.git"
	},
	"keywords": [
		"imgur",
		"api",
		"client"
	],
	"author": "CharlesRA",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/CharlesRA/imgur.js/issues"
	},
	"homepage": "https://github.com/CharlesRA/imgur.js#readme",
	"dependencies": {
		"request": "^2.88.2",
		"request-promise": "^4.2.6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"dotenv": "^8.2.0",
		"jsdoc": "^3.6.5",
		"mocha": "^8.1.3",
		"prettier": "^2.1.1"
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": false,
		"endOfLine": "auto",
		"htmlWhitespaceSensitivity": "ignore",
		"insertPragma": false,
		"jsxBracketSameLine": false,
		"jsxSingleQuote": false,
		"proseWrap": "never",
		"quoteProps": "consistent",
		"requirePragma": false,
		"semi": true,
		"singleQuote": false,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true,
		"vueIndentScriptAndStyle": false
	}
}
